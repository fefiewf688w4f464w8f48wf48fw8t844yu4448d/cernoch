local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/fefiewf688w4f464w8f48wf48fw8t844yu4448d/NEVERLOSE_ui_lib/main/repost_"))()a:Theme("nightly")local b=a:AddWindow("Astral Zap","")local c=a:Notification()c.MaxNotifications=6;local d=b:AddTab('Settings')local e=d:AddSection('Main',"left")local f=d:AddSection('UI',"right")getgenv().NameAZ=false;getgenv().BoxAZ=false;e:AddToggle("Name ESP",false,function(g)if getgenv().NameAZ==false then getgenv().NameAZ=true else getgenv().NameAZ=false end end)e:AddToggle("Box ESP",false,function(g)if getgenv().BoxAZ==false then getgenv().BoxAZ=true else getgenv().BoxAZ=false end end)f:AddButton("Destroy UI",function()for h,i in pairs(game.CoreGui:GetChildren())do if i.Name=="ScreenGui"and i:FindFirstChild("Frame")then if i.Frame:FindFirstChild("HeadName")and i.Frame:FindFirstChild("HeadName").Text=="Astral Zap"then i:Destroy()end end end end)local j=Instance.new("Folder")j.Name="DrawingLib"j.Parent=game.CoreGui;spawn(function()function addESP(k,l)local m=nil;local n=Instance.new("ScreenGui")local o=Instance.new("TextLabel")n.Name="Text"n.Parent=j;n.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;o.Parent=n;o.BackgroundColor3=Color3.fromRGB(255,255,255)o.BackgroundTransparency=1.000;o.Size=UDim2.new(0.00100000005,0,0.00100000005,0)o.Font=Enum.Font.SourceSans;o.TextColor3=Color3.fromRGB(255,255,255)o.TextSize=12.000;o.TextStrokeTransparency=0.000;o.Text=l;function drawingUpdater()m=game:GetService("RunService").RenderStepped:Connect(function()if k then local p,q=workspace.CurrentCamera:WorldToViewportPoint(k.Position)local r,s=workspace.CurrentCamera:WorldToViewportPoint(k.Size)if q and getgenv().NameAZ==true then o.Visible=true;o.Position=UDim2.new(0,p.X,0,p.Y-50)o.TextColor3=Color3.new(255,0,0)else o.Visible=false end end end)partRemovedConnection=k.AncestryChanged:Connect(function(t,u)if not u then n:Destroy()m:Disconnect()if not game.Players:FindFirstChild(k.Parent.Name)then partRemovedConnection:Disconnect()end end end)end;drawingUpdater()end;for h,i in pairs(game.Players:GetPlayers())do if i.Character and i.Character:FindFirstChild("Head")and i.Character:FindFirstChild("HumanoidRootPart")and i.Name~=game.Players.LocalPlayer.Name then addESP(i.Character.Head,i.Name)end;i.CharacterAdded:Connect(function(v)if i.Name~=game.Players.LocalPlayer.Name then if v:WaitForChild("HumanoidRootPart")and v:WaitForChild("Head")then addESP(v.Head,i.Name)end end end)end;game.Players.PlayerAdded:Connect(function(i)i.CharacterAdded:Connect(function(v)if i.Name~=game.Players.LocalPlayer.Name then if v:WaitForChild("HumanoidRootPart")and v:WaitForChild("Head")then addESP(v.Head,i.Name)end end end)end)end)local function w(x)local y=Instance.new("ScreenGui")local z=Instance.new("Frame")local A=Instance.new("UIStroke")y.Parent=j;y.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;y.ResetOnSpawn=false;y.Name="myBox"z.Parent=y;z.BackgroundColor3=Color3.fromRGB(255,255,255)z.BorderColor3=Color3.fromRGB(0,0,0)z.BorderSizePixel=0;z.Visible=false;z.Transparency=1;A.Parent=z;A.ApplyStrokeMode=Enum.ApplyStrokeMode.Border;A.Color=Color3.fromRGB(255,0,0)local B=game:GetService("RunService").RenderStepped:Connect(function()local C,D=workspace.CurrentCamera:WorldToViewportPoint(x.Position)local E=x.Size;local F=workspace.CurrentCamera:WorldToViewportPoint(x.Position-E/2)local G=workspace.CurrentCamera:WorldToViewportPoint(x.Position+E/2)local H=F.X;local I=F.Y;local J=G.X-F.X;local K=G.Y-F.Y;if D and getgenv().BoxAZ==true then z.Visible=true;z.Size=UDim2.new(0,J*2,0,K*2.5)z.Position=UDim2.new(0,H-J/2,0,I-K/2-35)else z.Visible=false end end)partRemovedConnection=x.AncestryChanged:Connect(function(t,u)if not u then y:Destroy()B:Disconnect()if not game.Players:FindFirstChild(x.Parent.Name)then partRemovedConnection:Disconnect()end end end)end;for h,i in pairs(game.Players:GetPlayers())do if i.Character then if i.Character.HumanoidRootPart and i.Name~=game.Players.LocalPlayer.Name then w(i.Character.HumanoidRootPart)end end;i.CharacterAdded:Connect(function(v)if i.Name~=game.Players.LocalPlayer.Name then if v:WaitForChild("HumanoidRootPart")then w(i.Character.HumanoidRootPart)end end end)end;game.Players.PlayerAdded:Connect(function(i)i.CharacterAdded:Connect(function(v)if i.Name~=game.Players.LocalPlayer.Name then if v:WaitForChild("HumanoidRootPart")then w(i.Character.HumanoidRootPart)end end end)end)
